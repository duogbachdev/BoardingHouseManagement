	<parameter name="maHoaDon" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT        dbo.HoaDon.Id, dbo.PhongTro.TenPhong, dbo.HoaDon.NgayBatDau, dbo.HoaDon.NgayKetThuc, dbo.HoaDon.SoDienCu, dbo.HoaDon.SoDienMoi, dbo.HoaDon.SoNuocCu, dbo.HoaDon.SoNuocMoi, dbo.HoaDon.KhauTru, 
                         dbo.HoaDon.TienNo, dbo.HoaDon.TongCong, dbo.HoaDon.NgayTao, dbo.HopDong.GiaPhong, dbo.HopDong.GiaInternet, dbo.HopDong.GiaRac, dbo.NguoiDung.DienThoai, dbo.NguoiDung.HoTen, dbo.HoaDon.Trash
FROM            dbo.HoaDon INNER JOIN
                         dbo.HopDong ON dbo.HoaDon.IdHopDong = dbo.HopDong.Id AND dbo.HoaDon.IdHopDong = dbo.HopDong.Id AND dbo.HoaDon.IdHopDong = dbo.HopDong.Id AND dbo.HoaDon.IdHopDong = dbo.HopDong.Id INNER JOIN
                         dbo.NguoiDung ON dbo.HopDong.IdNguoiDung = dbo.NguoiDung.Id AND dbo.HopDong.IdNguoiDung = dbo.NguoiDung.Id AND dbo.HopDong.IdNguoiDung = dbo.NguoiDung.Id AND 
                         dbo.HopDong.IdNguoiDung = dbo.NguoiDung.Id INNER JOIN
                         dbo.PhongTro ON dbo.HopDong.IdMaPhongTro = dbo.PhongTro.Id AND dbo.HopDong.IdMaPhongTro = dbo.PhongTro.Id AND dbo.HopDong.IdMaPhongTro = dbo.PhongTro.Id AND 
                         dbo.HopDong.IdMaPhongTro = dbo.PhongTro.Id AND dbo.NguoiDung.Id = dbo.PhongTro.IdNguoiDung AND dbo.NguoiDung.Id = dbo.PhongTro.IdNguoiDung AND dbo.NguoiDung.Id = dbo.PhongTro.IdNguoiDung AND 
                         dbo.NguoiDung.Id = dbo.PhongTro.IdNguoiDung
WHERE  dbo.HoaDon.Trash = 1  AND dbo.HoaDon.Id = $P{maHoaDon}]]>
	</queryString>
